{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "extends": [
    "eslint:recommended",
    "google",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prefer-const": "warn"
  }
}
