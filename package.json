{
  "name": "@staffbase/plugins-client-sdk",
  "version": "0.2.0-beta5",
  "description": "Staffbase plugins client SDK for JavaScript",
  "main": "dist/plugins-client-sdk.js",
  "module": "dist/plugins-client-sdk.esm.js",
  "browser": "dist/plugins-client-sdk.umd.js",
  "scripts": {
    "pretest": "npm run build",
    "build": "rollup -c",
    "prebuild": "npm run lint",
    "dev": "rollup -c -w",
    "test": "npm run test-unit",
    "test-unit": "npx mocha --recursive --compilers js:babel-register -r jsdom-global/register -- test",
    "test-dev": "npx mocha --recursive --compilers js:babel-register -r jsdom-global/register --watch -- test",
    "lint": "npm run lint-code && npm run lint-size",
    "lint-code": "npx eslint src test",
    "lint-size": "npx size-limit",
    "fix": "eslint --fix 'src/**/*.js' 'test/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Staffbase/plugins-client-sdk.git"
  },
  "keywords": [
    "staffbase",
    "client",
    "sdk",
    "javascript",
    "plugin",
    "api"
  ],
  "author": {
    "name": "Staffbase GmbH",
    "email": "",
    "url": "https://staffbase.com/"
  },
  "contributors": [
    "Stefan Staude <stefan.staude@staffbase.com>",
    "Vitaliy Ivanov <vitaliy@staffbase.com>",
    "Daniel Gro√üe <daniel.grosse@staffbase.com>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Staffbase/plugins-client-sdk/issues"
  },
  "homepage": "https://github.com/Staffbase/plugins-client-sdk#readme",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "size-limit": [
    {
      "path": "src/main.js",
      "limit": "40 KB"
    }
  ],
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "compare-versions": "^3.3.0",
    "eslint": "^4.1.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "loglevel": "^1.6.1",
    "mocha": "^3.5.3",
    "mocha-jsdom": "^2.0.0",
    "prettier": "^1.13.5",
    "prettier-eslint": "^8.8.1",
    "rollup": "^0.60.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-license": "0.6.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "4.0.0",
    "sinon": "^6.0.0",
    "size-limit": "^0.18.2"
  }
}
